{
    "name": "adlogix/confluence-rest-php-client",
    "version": "0.1.0",
    "license": "MIT",
    "description": "Confluence API PHP Client",
    "require": {
        "php": ">=5.5.0",
        "adlogix/guzzle-atlassian-connect-middleware": "dev-feature/Handle_Spaces_in_URI",
        "jms/serializer": "^1.3",
        "symfony/yaml": "^3.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adlogix/guzzle-atlassian-connect-middleware"
        }
    ],
    "require-dev": {
        "silex/silex": "^2.0",
        "guzzlehttp/guzzle": "^6.2",
        "twig/twig": "^1.26",
        "phpunit/phpunit": "^5.6"
    },
    "authors": [
        {
            "name": "Cedric Michaux",
            "email": "cedric@adlogix.eu"
        }
    ],
    "autoload": {
        "psr-0": {
            "Adlogix\\Confluence\\Client": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Adlogix\\Tests\\Confluence\\Client": "test/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "For using this library as it was created for Guzzle6. But you should be able to use it with any PSR-7 HTTP Client"
    }
}
